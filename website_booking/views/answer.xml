<odoo>
    <data>

        <record model="ir.ui.view" id="view_event_question_product_form">
            <field name="name">event.question.product.form</field>
            <field name="model">event.question</field>
            <field name="inherit_id" ref="website_event_questions.view_event_question_form"/>
            <field name="arch" type="xml">
                <xpath expr="//tree/field[@name='name']" position="after">
                        <field name="x_type_logement"/>
                        <field name="x_days"/>
                        <field name="x_product_ids" widget="many2many_tags"/>
                        <!--
                        <field name="x_url"/>
                        <field name="x_type_qty"/>
                        <field name="x_qty"/>
                        -->
                </xpath>
            </field>
        </record>

    </data>
</odoo>
