<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
<!--
'|',
                                            ('company_id', '=', False),
                                            ('company_id', 'child_of', [user.company_id.id]),
-->
      <record id="event.event_event_company_rule" model="ir.rule">
          <field name="domain_force">[]</field>
      </record>
    </data>


    <data noupdate="1">

      <record id="email_template_forget_me" model="mail.template">
          <field name="name">Request for forget ME</field>
          <field name="email_from">${object.email}</field>
          <field name="subject">Request for forget ME</field>
          <field name="email_to">${object.company_id.email}</field>
          <field name="model_id" ref="base.model_res_partner"/>
          <field name="auto_delete" eval="True"/>
          <field name="lang">en_US</field>
          <field name="body_html"><![CDATA[
               <p>Dear All,</p>
              <p></p>
              <p>The partner "${object.name}", please forget me !</p>
             <p></p>
             <p>Best Regards,</p>
          ]]></field>
      </record>

      <record id="mail_activity_urgent" model="mail.activity.type">
          <field name="name">Urgent</field>
          <field name="icon">fa-hourglass</field>
          <field name="days">-1</field>
      </record>

       <record id="default_booking_config" model="booking.config">
            <field name="name">My config</field>
            <field name="tz">UTC</field>
        </record>

        <record id="categ_booking" model="product.uom.categ">
            <field name="name">Booking Time</field>
        </record>

       <record id="unit_time_booking_second" model="product.uom">
            <field name="name">Second(s)</field>
            <field name="uom_type">reference</field>
            <field name="category_id" ref="categ_booking"/>
        </record>

        <record id="unit_time_booking_minute" model="product.uom">
            <field name="name">Minute(s)</field>
            <field name="uom_type">bigger</field>
            <field name="category_id" ref="categ_booking"/>
            <field name="factor_inv" eval="60.0"/>
        </record>

        <record id="unit_time_booking_hour" model="product.uom">
            <field name="name">Hour(s)</field>
            <field name="uom_type">bigger</field>
            <field name="category_id" ref="categ_booking"/>
            <field name="factor_inv" eval="3600.0"/>
        </record>

        <record id="unit_time_booking_day" model="product.uom">
            <field name="name">Night(s)</field>
            <field name="uom_type">bigger</field>
            <field name="category_id" ref="categ_booking"/>
            <field name="factor_inv" eval="86400.0"/>

        </record>

        <record id="get_doubly_room_cron" model="ir.cron">
            <field name="name">Search doubly reserved room</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model_id" ref="model_sale_order_line"/>
            <field name="state">code</field>
            <field name="code">model._cron_get_doubly_sale()</field>
        </record>


        <record id="get_new_membership_cron" model="ir.cron">
            <field name="name">Membership : New</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model_id" ref="model_sale_subscription"/>
            <field name="state">code</field>
            <field name="code">model._cron_membership_today()</field>
        </record>


        <record id="get_close_membership_cron" model="ir.cron">
            <field name="name">Membership : Close</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model_id" ref="model_sale_subscription"/>
            <field name="state">code</field>
            <field name="code">model._cron_membership_clean()</field>
        </record>

        <record id="generate_barcode_partner_cron" model="ir.cron">
            <field name="name">Membership : Generate barcode</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model_id" ref="model_res_partner"/>
            <field name="state">code</field>
            <field name="code">model._cron_membership_barcode()</field>
        </record>


		<record id="generate_user_partner_cron" model="ir.cron">
            <field name="name">Go-Live : Generate User of partner</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">30</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model_id" ref="model_res_partner"/>
            <field name="state">code</field>
            <field name="code">model._cron_generate_userportal()</field>
        </record>

        <record id="double_partner_email_cron" model="ir.cron">
            <field name="name">Double email for partner</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">5</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model_id" ref="model_res_partner"/>
            <field name="state">code</field>
            <field name="code">model._cron_search_double_email()</field>
        </record>

        <record id="sale_order_cancel_cron" model="ir.cron">
            <field name="name">Cancel old Sale Order</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">2</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model_id" ref="model_sale_order"/>
            <field name="state">code</field>
            <field name="code">model._cron_cancel_sale_order()</field>
        </record>

      </data>

</odoo>
