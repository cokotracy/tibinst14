<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <template inherit_id="website.snippets" id="snippets_calendar">
            <xpath expr="//div[@id='snippet_feature']//div[@class='o_panel_body']" position="inside">
                <t t-snippet="website_calendar_snippet.s_calendar" t-thumbnail="/website_calendar_snippet/static/src/img/blocks/calendar.png"/>
            </xpath>
       </template>

       <template inherit_id="website.snippets" id="snippets_institut">
           <xpath expr="//div[@id='snippet_feature']//div[@class='o_panel_body']" position="inside">
               <t t-snippet="website_calendar_snippet.s_instituut" t-thumbnail="/website_calendar_snippet/static/src/img/YTL_logo.png"/>
           </xpath>
      </template>

        <template name="Calendar" id="s_calendar">
             <section class="hidden-xs s_calendar">
                 <div class="hidden-xs container">
                       <div class="hidden-xs oe_snippet_body calendar_block panel panel-default">
                         <div class="panel-body">
                               <div class="calendar"></div>
                             </div>
                       </div>
                   </div>
              </section>
        </template>


